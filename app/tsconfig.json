{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "typeRoots": ["./src/@types"],
    "paths": {
      "@assets/*":["./src/assets/*"],
      "@components/*":["./src/components/*"],
      "@routes/*":["./src/routes/*"],
      "@screens/*":["./src/screens/*"],
      "@storage/*":["./src/storage/*"],
      "@theme/*":["./src/theme/*"],
      "@utils/*":["./src/utils/*"],
      "@services/*":["./src/services/*"],
      "@dtos/*":["./src/dtos/*"],
    }
  }
}
